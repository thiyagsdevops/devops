{
    "Create_Service":{
        "Script": "./build/service.create.Jenkinsfile",
        "Parameters": [
            {
                "Name":"docker_image_version",
                "Type":"String",
                "Default": "latest",
                "Description":"Which image version should be used."
            },
            {
                "Name":"service",
                "Type":"Choice",
                "Choices":["cadvisor","nexus","sonarqube","tomcat","elk","selenium", "awx"]
            }
        ]
    },
    "Destroy_Service":{
        "Script": "./build/service.destroy.Jenkinsfile",
        "Parameters": [
            {
                "Name":"service",
                "Type":"Choice",
                "Choices":["cadvisor","nexus","sonarqube","tomcat","elk","selenium", "awx"]
            }
        ]
    }
}